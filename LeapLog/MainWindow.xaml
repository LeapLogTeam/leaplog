<Window x:Class="LeapLog.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LeapLog"
        mc:Ignorable="d"
        Title="Dashboard" Height="700" Width="800">
    <Grid Margin="0,63,0,0">
        <TabControl HorizontalAlignment="Left" Height="547" Margin="22,0,0,0" VerticalAlignment="Center" Width="752">
            <TabItem Header="Journal" Margin="-2,-2,-36,0">
                <Grid Background="#FFE5E5E5">
                    <local:Journal Margin="0,0,10,0"></local:Journal>
                </Grid>
            </TabItem>
            <TabItem Header="T-Accounts" Margin="37,-2,-74,0">
                <Grid Background="#FFE5E5E5">
                    <local:T_Accounts></local:T_Accounts>
                </Grid>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
